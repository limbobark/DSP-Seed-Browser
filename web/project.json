{
    "name": "web",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "web",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm build",
                "cwd": "web"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm dev",
                "cwd": "web"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "web"
            }
        }
    }
}